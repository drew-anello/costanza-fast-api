{
    "family": "costanza-task-def",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "costanza-api-service",
            "image": "your-docker-image-url",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "ENV_VAR_NAME",
                    "value": "value"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}